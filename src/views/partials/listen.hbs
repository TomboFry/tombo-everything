<div class="sectioncontainer">
	<div class="row">
		<span class="left">🎵</span>
		<span class="right"><strong>{{data.title}}</strong></span>
	</div>
	<div class="row">
		<span class="left">💿</span>
		<span class="right">{{data.album}}</span>
	</div>
	<div class="row">
		<span class="left">👤</span>
		<span class="right">{{data.artist}}</span>
	</div>
	<div class="row">
		<span class="left"></span>
		<time datetime="{{data.created_at}}" class="right">
			<small>
			{{#if showlink }}
				<a href="/music/{{data.id}}">{{data.timeago}}</a>
			{{else}}
				{{data.timeago}}
			{{/if}}
			</small>
		</time>
	</div>
</div>
