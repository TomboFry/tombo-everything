<div class="page--data__header">
	<a href="/">&lt;- Back</a>
	<h2>Log an episode:</h2>
</div>

<p>Pick a TV series:</p>

<form class="scrobble" action="/tv/episode" method="GET">
	<select name="seriesId">
	{{#each seriesList as |series| }}
		<option value="{{series.id}}">{{series.title}}</option>
	{{/each}}
	</select>

	<input type="submit" value="Pick an episode" />
</form>
